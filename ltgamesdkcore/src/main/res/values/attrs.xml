<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CountDownButton">
        <attr name="millisinfuture" format="integer" />
        <attr name="countdowninterva" format="integer" />
        <attr name="normalColor" format="color" />
        <attr name="countDownColor" format="color" />
    </declare-styleable>
    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer" />
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ProgressView">
        <attr name="dot_color" format="color" />
        <attr name="dot_count" format="integer" />
    </declare-styleable>
</resources>